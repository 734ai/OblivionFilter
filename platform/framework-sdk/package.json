{
  "name": "@oblivionfilter/sdk",
  "version": "2.0.0",
  "description": "OblivionFilter SDK for web frameworks - Enterprise-grade privacy filtering integration",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
