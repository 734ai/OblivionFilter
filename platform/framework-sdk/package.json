    "prepublishOnly": "npm run build",
    "docs": "typedoc src/index.ts"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "@angular/core": ">=12.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "crypto-js": "^4.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
